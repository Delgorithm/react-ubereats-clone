#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/metro/src/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/metro/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules:/Users/delgorithm/Documents/Projets/react-projects/node_modules:/Users/delgorithm/Documents/Projets/node_modules:/Users/delgorithm/Documents/node_modules:/Users/delgorithm/node_modules:/Users/node_modules:/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/metro/src/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/metro/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules:/Users/delgorithm/Documents/Projets/react-projects/node_modules:/Users/delgorithm/Documents/Projets/node_modules:/Users/delgorithm/Documents/node_modules:/Users/delgorithm/node_modules:/Users/node_modules:/node_modules:/Users/delgorithm/Documents/Projets/react-projects/react-ubereats-clone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../metro/src/cli.js" "$@"
else
  exec node  "$basedir/../metro/src/cli.js" "$@"
fi
